@model iMenyn.Web.Areas.Admin.ViewModels.AllEnterprisesViewModel

@{
    ViewBag.Title = "Modifierade menyer";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Modifierade menyer</h2>

@foreach(var enterprise in Model.Enterprises)
{
    @Html.ActionLink(enterprise.Name, "ModifiedMenu", "Admin", new {enterpriseKey = enterprise.Key},null)
}